defmodule AdventOfCode.Y<%= assigns.year %>.D<%= assigns.day %> do
  use AdventOfCode.Puzzle, year: <%= assigns.year %>, day: <%= assigns.day %>

  @impl true
  def title, do: "<%= assigns.title %>"

  @impl true
  def solve(input) do
    input
    |> then(&({solve_1(&1), solve_2(&1)}))
  end

  @impl true
  def parse(input) do
    input
  end

  defp solve_1(_input) do
    nil
  end

  defp solve_2(_input) do
    nil
  end
end
